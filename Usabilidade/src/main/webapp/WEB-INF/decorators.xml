<?xml version="1.0" encoding="UTF-8"?>

<decorators defaultdir="/WEB-INF/jsp/decorators">
	<decorator name="blank" page="blank.jsp">
		<pattern>/tarefa/loadtaskuser</pattern>
		<pattern>/teste/responder/pergunta/*</pattern>
	</decorator>
	<decorator name="testeUser" page="testeUser.jsp">
		<pattern>/tarefa/loadactionuser</pattern>
	</decorator>
	<decorator name="datamining" page="datamining.jsp">
		<pattern>/datamining/*</pattern>
	</decorator>
	<decorator name="templates" page="templates.jsp">
		<pattern>/templates/*</pattern>
	</decorator>
	<decorator name="main" page="main.jsp">
		<pattern>/*</pattern>
	</decorator>
</decorators>
